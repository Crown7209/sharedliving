{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:with-codegen": "yarn codegen && next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "dev:with-codegen": "concurrently \"yarn codegen:watch\" \"next dev\"",
    "typecheck": "tsc --noEmit",
    "validate": "yarn lint && yarn typecheck && yarn codegen"
  },
